{
    "name": "rufflib-expect",
    "version": "1.0.1",
    "description": "A RuffLIB library for unit testing rough and sketchy JavaScript apps.",
    "main": "docs/dist/rufflib-expect.es.js",
    "type": "module",
    "scripts": {
        "build": "rollup -c",
        "test:docs": "node docs/run-nodejs-tests.js # test docs/ using NodeJS",
        "test:src": "node src/run-nodejs-tests.js # test src/ using NodeJS",
        "test": "[ -z $npm_config_src ]&&D=docs||D=src;npm run test:$D"
    },
    "keywords": [
        "RuffLIB",
        "vanilla",
        "javascript",
        "unit-test",
        "testing",
        "test-framework"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/richplastow/rufflib-expect.git"
    },
    "author": "Rich Plastow <rich@richplastow.com> (https://richplastow.com)",
    "license": "MIT",
    "homepage": "https://richplastow.com/rufflib-expect",
    "devDependencies": {
        "@babel/preset-env": "^7.18.6",
        "@rollup/plugin-babel": "^5.3.1",
        "rollup-plugin-terser": "^7.0.2"
    }
}
